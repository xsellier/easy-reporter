{
  "name": "easy-error-reporter",
  "version": "0.1.0",
  "description": "Crash report API",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "rm -rf public/ && concurrently --kill-others \"./start.sh\" \"vue-cli-service build --watch\"",
    "lint": "node_modules/.bin/standard && vue-cli-service lint src/",
    "build": "rm -rf public/ && vue-cli-service build",
    "start-server": "mkdir -p .data && chmod -R a+w .data/ && node lib/index",
    "start": "sh ./start.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend": "vue-cli-service build --watch",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "keywords": [
    "godot",
    "error",
    "reporter",
    "uploader",
    "API"
  ],
  "author": "Xavier Sellier",
  "license": "MIT",
  "dependencies": {
    "@firebase/app": "^0.6.29",
    "@firebase/storage": "^0.6.2",
    "axios": "^0.19.0",
    "boom": "^7.3.0",
    "config": "^3.2.4",
    "db-migrate": "^0.11.12",
    "db-migrate-sqlite3": "^0.4.0",
    "firebase": "^8.9.1",
    "glob": "^7.1.6",
    "hapi": "^17.8.5",
    "hapi-auth-bearer-token": "^6.1.2",
    "inert": "^5.1.3",
    "joi": "^14.3.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.11",
    "request": "^2.88.0",
    "sqlite3": "^4.2.0",
    "uuid": "^3.3.3",
    "vue": "^2.6.11",
    "vue-json-viewer": "^2.2.8",
    "vuetify": "^1.5.21",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "concurrently": "^4.1.2",
    "node-sass": "^7.0.0",
    "ora": "^5.1.0",
    "sass-loader": "^7.3.1",
    "standard": "^12.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "14.x"
  },
  "standard": {
    "ignore": [
      "dist/",
      "migrations/"
    ]
  }
}
