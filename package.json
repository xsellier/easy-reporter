{
  "name": "easy-error-reporter",
  "version": "0.1.0",
  "description": "Crash report API",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prepare": "npm run build",
    "dev": "NODE_ENV=dev rm -rf public/ && concurrently --kill-others \"./start.sh\" \"vite build --watch\"",
    "lint": "node_modules/.bin/standard",
    "build": "rm -rf public/ && vite build",
    "start-server": "mkdir -p .data && chmod -R a+w .data/ && node lib/index",
    "start": "sh ./start.sh",
    "frontend": "vite build --watch",
    "preview": "vite preview"
  },
  "keywords": [
    "godot",
    "error",
    "reporter",
    "uploader",
    "API"
  ],
  "author": "Xavier Sellier",
  "license": "MIT",
  "dependencies": {
    "@firebase/app": "^0.6.29",
    "@firebase/storage": "^0.11.1",
    "@hapi/boom": ">=7.x.x",
    "@hapi/hapi": ">=19.x.x",
    "autoprefixer": "^10.4.13",
    "axios": "^0.21.4",
    "config": "^3.2.4",
    "db-migrate": "^0.11.13",
    "db-migrate-sqlite3": "^1.0.2",
    "discord.js": "^14.0.0",
    "firebase": "^8.9.1",
    "glob": "^7.1.6",
    "hapi-auth-bearer-token": "*",
    "inert": "^5.1.3",
    "joi": "^17.9.2",
    "nodemailer": "^6.4.11",
    "postcss": "^8.4.21",
    "sqlite3": "^5.1.0",
    "superagent": "^8.0.9",
    "tailwindcss": "^3.2.4",
    "uuid": "^7.0.0",
    "vue": "^3.2.38",
    "vuetify": "^3.1.3",
    "webfontloader": "^1.0.0",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "concurrently": "^4.1.2",
    "node-sass": "^4.14.1",
    "ora": "^5.1.0",
    "sass-loader": "^7.3.1",
    "standard": "^17.0.1",
    "vite": "^4.1.1",
    "vite-plugin-vuetify": "^1.0.0-alpha.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "22.x"
  },
  "standard": {
    "ignore": [
      "dist/",
      "migrations/"
    ]
  }
}
